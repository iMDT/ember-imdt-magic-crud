<div>
  {{#each definitions as |definition|}}
    {{magic-input
      model=model
      attribute=definition.attribute
      label=definition.label
      type=definition.type
      selectContent=definition.selectContent
      optionValuePath=definition.selectValuePath
      optionLabelPath=definition.selectLabelPath
      errors=errors
      submitted=submitted}}
  {{/each}}
</div>
